<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.view.PhotoDisplayController">
   <children>
      <ImageView fx:id="photo" fitHeight="478.0" fitWidth="481.0" layoutX="62.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../Documents/tom-nook-moneyjpg.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="previousButton" layoutX="554.0" layoutY="297.0" mnemonicParsing="false" onAction="#displayPrevious" text="-&gt;" />
      <Button fx:id="nextButton" layoutX="16.0" layoutY="297.0" mnemonicParsing="false" onAction="#displayNext" text="&lt;-" />
      <Button fx:id="logoutButton" layoutX="508.0" layoutY="21.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <Button fx:id="backButton" layoutX="458.0" layoutY="21.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Button fx:id="captionButton" layoutX="33.0" layoutY="544.0" mnemonicParsing="false" onAction="#showEditCaptionPopup" text="Edit"/>
      <Label fx:id="photoCaption" layoutX="75.0" layoutY="548.0" text="Full Caption">
         <font>
            <Font name="System Italic" />
         </font>
      </Label>
      <Button fx:id="tagsButton" layoutX="33.0" layoutY="580.0" mnemonicParsing="false" onAction="#showEditTagsPopup" text="Edit"/>
      <Label fx:id="photoTags" layoutX="75.0" layoutY="584.0" text="Example Tag: Example Tag Value">
         <font>
            <Font name="System Italic" />
         </font>
      </Label>
      <Label fx:id="photoDate" layoutX="75.0" layoutY="34.0" text="Date: MM/DD/YYYY">
         <font>
            <Font name="System Italic" />
         </font>
      </Label>
      <Button fx:id="copyButton" layoutX="246.0" layoutY="636.0" mnemonicParsing="false" onAction="#showCopyPhotoPopup" text="Copy" />
      <Button fx:id="moveButton" layoutX="300.0" layoutY="636.0" mnemonicParsing="false" onAction="#showMovePhotoPopup" text="Move" />
   </children>
</AnchorPane>
