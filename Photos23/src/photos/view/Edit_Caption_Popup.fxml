      <Label layoutX="28.0" layoutY="84.0" text="Caption"/>
      <Button fx:id="okButton" layoutX="220.0" layoutY="177.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="65.0" onAction="#editCaption" text="OK" />
