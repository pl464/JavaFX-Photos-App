<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="406.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.view.NewPhotoController">
   <children>
      <Label layoutX="23.0" layoutY="22.0" text="New Photo">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextField fx:id="filePath" layoutX="93.0" layoutY="96.0" prefHeight="39.0" prefWidth="188.0" />
      <Label layoutX="52.0" layoutY="102.0" text="File" />
      <Button fx:id="okButton" layoutX="232.0" layoutY="353.0" mnemonicParsing="false" onAction="#createPicture" prefHeight="39.0" prefWidth="65.0" text="OK" />
      <Label layoutX="20.0" layoutY="151.0" text="Caption" />
      <TextField fx:id="captionText" layoutX="93.0" layoutY="145.0" prefHeight="39.0" prefWidth="286.0" />
      <Button fx:id="browseButton" layoutX="288.0" layoutY="96.0" mnemonicParsing="false" onAction="#chooseFile" prefHeight="39.0" prefWidth="95.0" text="Browse..." />
      <MenuButton fx:id="tagMenu" layoutX="13.0" layoutY="197.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="79.0" text="Tag">
        <items>
        </items>
      </MenuButton>
      <TextField fx:id="tagValue" layoutX="102.0" layoutY="197.0" onMouseClicked="#setFieldEmpty" prefHeight="39.0" prefWidth="196.0" text="Tag_Value" />
      <Button fx:id="insertTagButton" layoutX="308.0" layoutY="197.0" mnemonicParsing="false" onAction="#addTag" prefHeight="39.0" prefWidth="71.0" text="Insert" />
      <ListView fx:id="tagDisplay" layoutX="93.0" layoutY="248.0" prefHeight="82.0" prefWidth="286.0" />
      <Button fx:id="cancelButton" layoutX="305.0" layoutY="353.0" mnemonicParsing="false" onAction="#closePopup" prefHeight="39.0" prefWidth="79.0" text="Cancel" />
      <Label layoutX="44.0" layoutY="248.0" text="Tags" />
   </children>
</AnchorPane>
